<Window x:Class="networking2.Upload"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Upload" Height="314" Width="324" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="46*" />
            <ColumnDefinition Width="256*" />
        </Grid.ColumnDefinitions>
        <Image Height="261" HorizontalAlignment="Left" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="278" Source="/networking2;component/Images/Fantastic4-Logo.jpg" Margin="12,2,0,0" Grid.ColumnSpan="2" />
        <Button Content="Upload" Height="39" HorizontalAlignment="Left" Margin="65,196,0,0" Name="upload_File" VerticalAlignment="Top" Width="92" Click="button1_Click" Grid.Column="1" />
        <TextBox Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="uploadFile" VerticalAlignment="Top" Width="269" Grid.ColumnSpan="2" />
        <ProgressBar Height="26" HorizontalAlignment="Left" Margin="12,164,0,0" Name="Progress" VerticalAlignment="Top" Width="269" Grid.ColumnSpan="2" Background="#FFE4A81D" Foreground="#FFF9F9F9" />
    </Grid>
</Window>
